config_version=5

[application]

config/name="Chess Tiles"
run/main_scene="res://Scenes/main.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")

[autoload]

SignalBus="*res://Scripts/Managers/signal_bus.gd"
ResourceManager="*res://Scripts/Managers/resource_manager.gd"
GameManager="*res://Scripts/Managers/game_manager.gd"
InputManager="*res://Scripts/Managers/input_manager.gd"
PieceSpawnManager="*res://Scripts/Managers/piece_spawn_manager.gd"
EndGameScorer="*res://Scripts/Managers/end_game_scorer.gd"

[debug]

gdscript/warnings/untyped_declaration=2

[display]

window/size/viewport_width=180
window/size/viewport_height=320
window/size/initial_position_type=2
window/size/window_width_override=720
window/size/window_height_override=1280
window/stretch/mode="viewport"
window/handheld/orientation=1

[dotnet]

project/assembly_name="Chess Tiles"

[file_customization]

folder_colors={
"res://Audio/": "red",
"res://Scenes/": "green",
"res://Scripts/": "purple",
"res://Sprites/": "orange",
"res://Themes/": "yellow",
